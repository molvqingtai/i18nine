#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/molvqingtai/Developer/code/i18nine/node_modules/.pnpm/tsdown@0.13.1_typescript@5.9.2/node_modules/tsdown/dist/node_modules:/Users/molvqingtai/Developer/code/i18nine/node_modules/.pnpm/tsdown@0.13.1_typescript@5.9.2/node_modules/tsdown/node_modules:/Users/molvqingtai/Developer/code/i18nine/node_modules/.pnpm/tsdown@0.13.1_typescript@5.9.2/node_modules:/Users/molvqingtai/Developer/code/i18nine/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/molvqingtai/Developer/code/i18nine/node_modules/.pnpm/tsdown@0.13.1_typescript@5.9.2/node_modules/tsdown/dist/node_modules:/Users/molvqingtai/Developer/code/i18nine/node_modules/.pnpm/tsdown@0.13.1_typescript@5.9.2/node_modules/tsdown/node_modules:/Users/molvqingtai/Developer/code/i18nine/node_modules/.pnpm/tsdown@0.13.1_typescript@5.9.2/node_modules:/Users/molvqingtai/Developer/code/i18nine/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsdown/dist/run.mjs" "$@"
else
  exec node  "$basedir/../tsdown/dist/run.mjs" "$@"
fi
